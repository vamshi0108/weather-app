		<![endif]-->

</head>


<body>
  <video class="video-background" autoplay muted loop type="video/mp4" src="/images/bgvideo.mp4"></video>
  <div class="site-content">
    <div class="site-header">
      <div class="container">
        <a href="/" class="branding">
          <img src="images/logo.png" alt="" class="logo">
          <div class="logo-type">
            <h1 class="site-title">VAYS</h1>
            <small class="site-description">Weather Application</small>
          </div>
        </a>

        <!-- Default snippet for navigation -->
        <div class="main-navigation">
          <button type="button" class="menu-toggle"><i class="fa fa-bars"></i></button>
          <ul class="menu">
            <li class="menu-item"><a href="/">Home</a></li>
            <li class="menu-item"><a href="news">News</a></li>
            <li class="menu-item"><a href="photos">Photos</a></li>
            <li class="menu-item current-menu-item"><a href="contact">Contact</a></li>
          </ul> <!-- .menu -->
        </div> <!-- .main-navigation -->

        <div class="mobile-navigation">
          <button type="button" class="menu-toggle"><i class="fa fa-bars"></i></button>
          <ul class="menu">
            <li class="menu-item"><a href="/">Home</a></li>
            <li class="menu-item"><a href="news">News</a></li>
            <li class="menu-item current-menu-item"><a href="live-cameras">Live cameras</a></li>
            <li class="menu-item"><a href="photos">Photos</a></li>
            <li class="menu-item"><a href="contact">Contact</a></li>
          </ul> <!-- .menu -->
        </div>

      </div>
    </div> <!-- .site-header -->

    <main class="main-content">
      {{!-- <div class="container">
        <div class="breadcrumb">
          <a href="/">Home</a>
          <span>Contact</span>
        </div>
      </div> --}}

      <div class="fullwidth-block">
        <div class="container">
          <div class="col-md-5">
            <div class="contact-details">
              <div class="map" data-latitude="-6.897789" data-longitude="107.621735"></div>
              <div class="contact-info">
                <address>
                  <img src="images/icon-marker.png" alt="">
                  <p>VAYS. <br>
                    Hyderabad, Telangana</p>
                </address>

                <a href="#"><img src="images/icon-phone.png" alt="">+91 9705530295</a>
                <a href="#"><img src="images/icon-envelope.png" alt="">vamshinagulapally2@gmail.com</a>
              </div>
            </div>
          </div>
          <div class="col-md-6 col-md-offset-1">
            <h2 class="section-title">Contact us</h2>
            <p class="contactError">If you have a suggestion or a comment, please use the form below to contact us.</p>
            <form action="#" class="contact-form">
              <div class="row">
                <div class="col-md-6"><input class="name" type="text" placeholder="Your name..." required></div>
                <div class="col-md-6"><input class="email" type="text" placeholder="Email Addresss..." required></div>
              </div>
              <div class="row">
                <div class="col-md-6"><input class="MSISDN" type="text" placeholder="Mobile No..." required></div>
                <div class="col-md-6"><input class="subject" type="text" placeholder="Subject..." required></div>
              </div>
              <textarea name="" class="message" placeholder="Message..." required></textarea>
              <div class="text-right">
                <input type="submit" placeholder="Send message">
              </div>
            </form>

          </div>
        </div>
      </div>

    </main> <!-- .main-content -->

    <footer class="site-footer">
      <div class="container">
        <div class="row">
          <div class="col-md-8">
            <form action="#" class="subscribe-form">
              <input type="text" placeholder="Enter your email to subscribe...">
              <input type="submit" value="Subscribe">
            </form>
          </div>
          <div class="col-md-3 col-md-offset-1">
            <div class="social-links">
              <a href="#"><i class="fa fa-facebook"></i></a>
              <a href="#"><i class="fa fa-twitter"></i></a>
              <a href="#"><i class="fa fa-google-plus"></i></a>
              <a href="#"><i class="fa fa-pinterest"></i></a>
            </div>
          </div>
        </div>

        <p class="colophon">Copyright 2020 VAYS. Designed by Vamshi Nagulapally. All rights reserved</p>
      </div>
    </footer> <!-- .site-footer -->
  </div>

  <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false&amp;language=en"></script>
  <script src="js/contact.js"></script>

</body>

</html>